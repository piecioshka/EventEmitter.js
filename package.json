    "repository": {
        "type": "git",
        "url": "git+https://github.com/piecioshka/super-event-emitter.git"
    },
    "engines": {
        "node": ">=6"
    },
    "devDependencies": {
        "auto-changelog": "^1.16.4",
        "jasmine": "^3.6.4",
        "json-loader": "^0.5.7",
        "nyc": "^15.1.0",
        "string-replace-loader": "^2.3.0",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12"
    },
    "files": [
        "dist",
        "src",
        "index.d.ts",
        "index.js",
        "package.json",
        "README.md"
    ],
    "keywords": [
        "super",
        "event",
        "emitter",
        "observer",
        "events",
        "mixin",
        "on",
